# Environment Variables Template
# Copy this file to .env and fill in your actual values

# Twilio Configuration
TWILIO_ACCOUNT_SID=your_twilio_account_sid_here
TWILIO_AUTH_TOKEN=your_twilio_auth_token_here
TWILIO_PHONE_NUMBER=your_twilio_phone_number_here
TWILIO_VALIDATION_PHONE=your_validation_phone_number_here
TWILIO_CALLBACK_URL=your_twilio_callback_url_here

# Email Configuration
MAIL_HOST=your_smtp_host_here
MAIL_PORT=587
MAIL_FROM=your_email_here
MAIL_PASSWORD=your_email_password_here
MAIL_TO=recipient_email_here

# Organization Configuration
ORGANIZATION_NAME=Your Organization Name
ORGANIZATION_EMAIL=your_organization_email_here

# API Security
API_PASSWORD=your_secure_api_password_here
